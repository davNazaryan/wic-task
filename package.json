  "bugs": {
    "url": "https://github.com/davNazaryan/wic-task/issues"
  },
  "homepage": "https://github.com/davNazaryan/wic-task#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1"
  }
